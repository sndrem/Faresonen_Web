{
  "name": "faresonen-api",
  "version": "0.0.1",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "babel-node ./bin/www",
    "clean": "rm -rf build bin && mkdir build bin",
    "build_entry_point": "babel -o ./bin/www ./src/bin/www -s",
    "build-babel": "babel -d ./build ./src -s && npm run build_entry_point",
    "build": "npm run clean && npm run build-babel",
    "raspberry": "babel-node ./bin/www",
    "deploy": "sshpass -f ~/.piePwd.txt scp -r build pi@192.168.1.57:/home/pi/documents/Faresonen_Web",
    "test": "jest --watchAll",
    "test:coverage": "nyc npm test",
    "test:unit": "mocha --recursive test/middleware test/models test/routes",
    "test:integration": "mocha --recursive test/integration"
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "axios": "^0.17.1",
    "body-parser": "^1.13.3",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "^1.3.3",
    "express": "^4.16.2",
    "fpl-api-node": "^2.2.4",
    "jade": "^1.11.0",
    "moment": "^2.21.0",
    "morgan": "^1.6.1",
    "request": "^2.88.0",
    "serve-favicon": "^2.3.0",
    "socket.io": "^2.0.4",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "debug": "^2.2.0",
    "grunt": "^1.0.3",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-develop": "^0.4.0",
    "load-grunt-tasks": "^3.2.0",
    "mocha": "^4.0.1",
    "nyc": "^10.0.0",
    "regenerator-runtime": "^0.11.1",
    "supertest": "^2.0.0",
    "time-grunt": "^1.2.1"
  },
  "apidoc": {
